{
  "name": "slotswapper",
  "version": "1.0.0",
  "description": "Peer-to-peer time-slot scheduling application",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "start": "node server/index.js"
  },
  "keywords": [
    "scheduling",
    "calendar",
    "swap"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.6.3"
  },
  "devDependencies": {
    "axios": "^1.13.2",
    "concurrently": "^8.2.2"
  }
}
